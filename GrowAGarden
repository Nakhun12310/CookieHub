for i, v in pairs(game.workspace.Farm:GetChildren()) do
    if v:IsA("Folder") then
        if v:FindFirstChild("Important") then
            if v.Important:FindFirstChild("Plants_Physical") then
                print("Plants_Physical Founded")
                local plants = v.Important.Plants_Physical
                print(plants.Name)
                for i, f in pairs(plants:GetChildren()) do
                    if f:IsA("Model") then
                        if f.Name == "Carrot" then
                            print("Carrot functions")
                            elseif f.Name == "Strawberry" then
                                print("strawberry functions")
                                elseif f.Name == "Blueberry" then
                                    print("blueberry functions")
                                    elseif f.Name == "Tomato" then
                                        print("tomato functions")
                                        elseif f.Name == "Corn" then
                                            print("corn functions")
                                            elseif f.Name == "Watermelon" then
                                                print("watermellon functions")
                                                elseif f.Name == "Pumpkin" then
                                                    print("pumpkin function")
                                                    elseif f.Name == "Apple" then
                                                        print("apple functions")
                                                        elseif f.Name == "Bamboo" then
                                                            print("bamboo functions")
                                                            elseif f.Name == "Coconut" then
                                                                print("coconut functions")
                                                                elseif f.Name == "Cactus" then
                                                                    print("cactus functinos")
                                                                    elseif f.Name == "DragonFruit" then
                                                                        print("df functions")
                                                                        elseif f.Name == "Mango" then
                                                                            print("mango functions")
                                                            
                        end
                    end
                end
            end
        end
    end
end
_G.autocollect = true

local function Apple ()
        for i,v in pairs(game.workspace.Farm:GetChildren()) do
        if v:FindFirstChild("Important") and v.Important:FindFirstChild("Plants_Physical") then
            if v.Important.Plants_Physical:FindFirstChild("Apple") and v.Important.Plants_Physical.Apple:FindFirstChild("Fruits") then
                for i, f in pairs(v.Important.Plants_Physical.Apple.Fruits:GetChildren()) do
                    if f.Name == "Apple" then
                        if f:FindFirstChild("2") then
                            local two = f:FindFirstChild("2")
                            --print(two)
                            if two:FindFirstChild("ProximityPrompt") then
                                --print("ProximityPrompt Founded")
                                fireproximityprompt(two.ProximityPrompt, 15000)
                                else
                                    print("ProximityPrompt is not founded or is not grown enough")
                            end
                        end
                    end
                end
            end
        end
    end
end

while _G.autocollect do
    Apple()
    task.wait(0.1)
end
